<% if false %>
    <% if action_name != "edit" %>
        <% grid.rows.times do |i| %>
            <div class="flex">
                <% grid.cols.times do |k| %>
                    <%= render 'grids/cell_show', space:
                    grid.get_space(i,k), ticket: grid.get_ticket( i, k), seat_number:
                    grid.get_index(i,k), event: grid.event %> 
                <% end %>
            </div>
        <% end %>
    <% else %>
        <% grid.rows.times do |i| %>
            <div class="flex">
                <% grid.cols.times do |k| %>
                    <%= render 'grids/cell_edit', space:
                    grid.get_space(i,k), ticket: grid.get_ticket(i, k) %> 
                <% end %>
            </div>
        <% end %>
    <% end %>
<% end %>
